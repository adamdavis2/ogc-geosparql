reqs11gx:feature-properties rdf:type 
                                     spec:Requirement ;
                            skos:definition "Implementations shall allow the properties geo:hasGeometry, geo:hasDefaultGeometry, geo:hasLength, geo:hasArea, geo:hasVolume geo:hasCentroid, geo:hasBoundingBox and geo:hasSpatialResolution to be used in SPARQL graph patterns"@de ;
                            skos:prefLabel "GeoSPARQL 1.1 Anforderung: Feature Properties"@de ;
                            prov:wasDerivedFrom reqs10gx:feature-properties .

reqs11gx:geojson-literal rdf:type 
                                  spec:Requirement ;
                         skos:definition "All geo:geoJSONLiteral instances shall consist of the Geometry objects as defined in the GeoJSON specification [GEOJSON]"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: GeoJSON Literal"@de .

reqs11gx:geojson-literal-empty rdf:type 
                                        spec:Requirement ;
                               skos:definition "An empty geo:kmlLiteral shall be interpreted as an empty geometry"@de ;
                               skos:prefLabel "GeoSPARQL 1.1 Anforderung: KML Literal Empty"@de .

reqs11gx:geojson-literal-srs rdf:type 
                                      spec:Requirement ;
                             skos:definition "RDFS Literals of type geo:geoJSONLiteral do not contain a SRS definition. All literals of this type shall, according to the GeoJSON specification, be encoded only in, and be assumed to use, the WGS84 geodetic longitude-latitude spatial reference system (http://www.opengis.net/def/crs/OGC/1.3/CRS84)"@de ;
                             skos:prefLabel "GeoSPARQL 1.1 Anforderung: GeoJSON Literal Default SRS"@de .

reqs11gx:geometry-as-dggs-function rdf:type 
                                            spec:Requirement ;
                                   skos:definition "Implementations shall support geof:asDGGS, as a SPARQL extension function"@de ;
                                   skos:prefLabel "GeoSPARQL 1.1 Anforderung: asDGGS Funktion"@de .

reqs11gx:geometry-as-dggs-literal rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementations shall allow the RDF property geo:asDGGS to be used in SPARQL graph patterns"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry asDGGS Literal"@de .

reqs11gx:geometry-as-geojson-function rdf:type 
                                               spec:Requirement ;
                                      skos:definition "Implementations shall support geof:asGeoJSON, as a SPARQL extension function"@de ;
                                      skos:prefLabel "GeoSPARQL 1.1 Anforderung: asGeoJSON Funktion"@de .

reqs11gx:geometry-as-geojson-literal rdf:type 
                                              spec:Requirement ;
                                     skos:definition "Implementations shall allow the RDF property geo:asGeoJSON to be used in SPARQL graph patterns"@de ;
                                     skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry asGeoJSON Literal"@de .

reqs11gx:geometry-as-gml-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementations shall support geof:asGML, as a SPARQL extension function"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: asGML Funktion"@de .

reqs11gx:geometry-as-kml-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementations shall support geof:asKML, as a SPARQL extension function"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: asKML Funktion"@de .

reqs11gx:geometry-as-kml-literal rdf:type 
                                          spec:Requirement ;
                                 skos:definition "Implementations shall allow the RDF property geo:asKML to be used in SPARQL graph patterns"@de ;
                                 skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry asKML Literal"@de .

reqs11gx:geometry-as-wkt-function rdf:type 
                                           spec:Requirement ;
                                  skos:definition "Implementations shall support geof:asWKT, as a SPARQL extension function"@de ;
                                  skos:prefLabel "GeoSPARQL 1.1 Anforderung: asWKT Funktion"@de .

reqs11gx:geometry-properties rdf:type 
                                      spec:Requirement ;
                             skos:definition "Implementations shall allow the properties geo:dimension, geo:coordinateDimension, geo:spatialDimension, geo:isEmpty, geo:isSimple, geo:hasSerialization to be used in SPARQL graph patterns"@de ;
                             skos:prefLabel "GeoSPARQL 1.1 Anforderung: Geometry Properties"@de ;
                             prov:wasDerivedFrom reqs10gx:geometry-properties .

reqs11gx:kml-literal rdf:type 
                              spec:Requirement ;
                     skos:definition "All geo:kmlLiteral instances shall consist of the Geometry objects as defined in the KML specification [OGCKML]"@de ;
                     skos:prefLabel "GeoSPARQL 1.1 Anforderung: KML Literal"@de .

reqs11gx:kml-literal-empty rdf:type 
                                    spec:Requirement ;
                           skos:definition "An empty geo:geoJSONLiteral shall be interpreted as an empty geometry"@de ;
                           skos:prefLabel "GeoSPARQL 1.1 Anforderung: GeoJSON Literal Empty"@de .

reqs11gx:kml-literal-srs rdf:type 
                                  spec:Requirement ;
                         skos:definition "The IRI http://www.opengis.net/def/crs/OGC/1.3/CRS84 shall be assumed as the SRS for geo:kmlLiterals that do not specify an explicit SRS IRI"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: KML Literal Default SRS"@de .

reqs11gx:query-functions rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementations shall support the functions geof:boundary geof:buffer, geof:convexHull, geof:coordinateDimension, geof:difference, geof:dimension, geof:distance, geof:envelope, geof:geometryType, geof:getSRID, geof:intersection, geof:is3D, geof:isEmpty, geof:isMeasured, geof:isSimple, geof:spatialDimension, geof:symDifference, geof:transform and geof:union as SPARQL extension functions, consistent with definitions of these functions in Simple Features [ISO19125-1], for non-DGGS geometry literals"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .

reqs11gx:query-functions-non-sf rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementations shall support the functions geof:area, geof:geometryN, geof:length, geof:maxX, geof:maxY, geof:maxZ, geof:minX, geof:minY, geof:minZ, geof:numGeometries and geof:projectTo as SPARQL extension functions which are defined in this standard, for non-DGGS geometry literals"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .

reqs11gx:query-functions-dggs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementations shall support the functions of Requirement 40 for DGGS geometry literals as SPARQL extension functions, consistent with definitions of these functions in Simple Features <<ISO19125-1>>, for non-DGGS geometry literals"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .    

reqs11gx:query-functions-non-sf-dggs rdf:type 
                                  spec:Requirement ;
                         skos:definition "Implementations shall support the functions of Requirement 41 for DGGS geometry literals as SPARQL extension functions which are defined in this standard, for non-DGGS geometry literals"@de ;
                         skos:prefLabel "GeoSPARQL 1.1 Anforderung: Query Funktionen"@de ;
                         prov:wasDerivedFrom reqs10gx:query-functions .                                                                       

reqs11gx:sa-functions rdf:type 
                               spec:Requirement ;
                      skos:definition "Implementations shall support geof:concaveHull, geof:boundingCircle, geof:union2, geof:concatLines, geof:centroid, geof:maxX, geof:maxY, geof:maxZ, geof:minX, geof:minY and geof:minZ as a SPARQL extension functions."@de ;
                      skos:prefLabel "GeoSPARQL 1.1 Anforderung: Spatial Aggregate Funktionen"@de .
